define i32 @test_while_return(i32 %n.0) {
entry:
    %n.addr.1 = alloca i32
    store i32 %n.0, i32* %n.addr.1
    %i.addr.2 = alloca i32
    store i32 0, i32* %i.addr.2
    br label %loop.header
loop.header:
    %t3 = load i32, i32* %n.addr.1
    %t4 = icmp slt i32 %t3, 0
    br i1 %t4, label %if.then, label %if.else
loop.body:
    %t9 = load i32, i32* %i.addr.2
    %t10 = add i32 %t9, 1
    store i32 %t10, i32* %i.addr.2
    br label %loop.header
loop.exit:
    %t11 = load i32, i32* %i.addr.2
    ret i32 %t11
if.then:
    %t5 = sub i32 0, 1
    ret i32 %t5
if.else:
    %t6 = load i32, i32* %i.addr.2
    %t7 = load i32, i32* %n.addr.1
    %t8 = icmp slt i32 %t6, %t7
    br label %if.merge
if.merge:
    br i1 %t8, label %loop.body, label %loop.exit
}

define void @main() {
entry:
    %result1.addr.0 = alloca i32
    %t1 = call i32 @test_while_return(i32 5)
    store i32 %t1, i32* %result1.addr.0
    %result2.addr.2 = alloca i32
    %t3 = sub i32 0, 1
    %t4 = call i32 @test_while_return(i32 %t3)
    store i32 %t4, i32* %result2.addr.2
    %t5 = load i32, i32* %result1.addr.0
    call void @printlnInt(i32 %t5)
    %t6 = load i32, i32* %result2.addr.2
    call void @printlnInt(i32 %t6)
    call void @exit(i32 0)
    ret void
}
