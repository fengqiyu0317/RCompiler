/*
Autoref test 6 (from semantic-1/autoref6): nested struct field access through reference
*/

struct Inner {
    value: i32
}

struct Outer {
    inner: Inner,
    count: i32
}

fn main() {
    let outer: Outer = Outer {
        inner: Inner { value: 42 },
        count: 5
    };
    let outer_ref: &Outer = &outer;
    let inner_value: i32 = outer_ref.inner.value;
    let count_value: i32 = outer_ref.count;
    printlnInt(inner_value + count_value);
    exit(0);
}
