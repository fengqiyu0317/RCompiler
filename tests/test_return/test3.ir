define i32 @outer() {
entry:
    %result.addr.0 = alloca i32
    %t1 = call i32 @outer$inner(i32 5)
    store i32 %t1, i32* %result.addr.0
    %t2 = load i32, i32* %result.addr.0
    %t3 = icmp sgt i32 %t2, 5
    br i1 %t3, label %if.then, label %if.else
if.then:
    %t4 = load i32, i32* %result.addr.0
    ret i32 %t4
if.else:
    br label %if.merge
if.merge:
    ret i32 1
}

define i32 @outer$inner(i32 %x.0) {
entry:
    %x.addr.1 = alloca i32
    store i32 %x.0, i32* %x.addr.1
    %t2 = load i32, i32* %x.addr.1
    %t3 = icmp sgt i32 %t2, 0
    br i1 %t3, label %if.then, label %if.else
if.then:
    %t4 = load i32, i32* %x.addr.1
    %t5 = mul i32 %t4, 2
    ret i32 %t5
if.else:
    br label %if.merge
if.merge:
    ret i32 0
}

define void @main() {
entry:
    %result.addr.0 = alloca i32
    %t1 = call i32 @outer()
    store i32 %t1, i32* %result.addr.0
    %t2 = load i32, i32* %result.addr.0
    call void @printlnInt(i32 %t2)
    call void @exit(i32 0)
    ret void
}
